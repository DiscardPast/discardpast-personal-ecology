/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.view_center_horizontal  {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.view_center_vertical {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}